{
    "version": 2,
    "controller": {},
    "workers": [
        {
            "type": "router",
            "realms": [
                {
                    "name": "realm1",
                    "roles": [
                        {
                            "name": "anonymous",
                            "permissions": [
                                {
                                    "uri": "*",
                                    "allow": {
                                        "call": true,
                                        "register": true,
                                        "publish": true,
                                        "subscribe": true
                                    },
                                    "disclose": {
                                        "caller": false,
                                        "publisher": false
                                    },
                                    "cache": true
                                }
                            ]
                        }
                    ]
                }
            ],
            "transports": [

                {
                  "type": "websocket",
                  "endpoint": {
                    "type": "tcp",
                    "interface": "127.0.0.1",
                    "port": 433,
                    "tls": {
                      "key": "./example.privkey.pem",
                      "certificate": "./example.cert.pem"
                    }
                  },
                  "url": "wss://127.0.0.1:433",
                  "debug": true
                },
                {
                  "type": "websocket",
                  "endpoint": {
                    "type": "tcp",
                    "interface": "127.0.0.1",
                    "port": 4433
                  },
                  "url": "ws://127.0.0.1:4433",
                  "debug": true
                }
            ]
        }
    ]
}
